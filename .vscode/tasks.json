{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "构建数据结构",
      "detail": "用于构建 OutdoorAerial 的数据结构 Dart 文件",
      "type": "shell",
      "command": [
        "mkdir",
        "build\\models;",
        "protoc",
        "-I .\\proto",
        "-I $env:GOOGLEAPIS",
        "--dart_out=.\\build\\models",
        ".\\proto\\program\\program.proto",
        ".\\proto\\station\\status.proto",
        ".\\proto\\station\\station.proto",
        ".\\proto\\programs.proto"
      ]
    },
    {
      "label": "清除数据结构",
      "detail": "用于清除生成的 OutdoorAerial 的 Dart 数据结构",
      "type": "shell",
      "command": ["cd .\\build;", "del * -Recurse -Force"]
    }
  ]
}
